programa {
    funcao inicio() {
        inteiro num, ndois
        escreva("Digite o primeiro número: ")
        leia(num)
        escreva("Digite o segundo número: ")
        leia(ndois)
        se (num>ndois) {
            escreva("O maior número é: ", num)
        } 
        senao {
            escreva("O maior número é: ", ndois)
        }
    }
}
